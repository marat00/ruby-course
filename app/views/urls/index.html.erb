<div class="row">
<div class="twelve columns">
  
<table>
  <tr><h3>Currently Registered</h3></tr>
  <tr>
    <th>Course</th>
    <th>Student</th>
    <th>Url</th>
  </tr>

  <%= will_paginate @students %>

  <% @students.each do |s| %>
    <% s.urls.each do |u| %>
      <tr>
        <td><%= s.course %></td>
        <td><%= s.name %></td>
        <td><a href="<%= u.url %>" title="<%= s.name %>"><%= u.url %></a></td>
      </tr>
    <% end %>
  <% end %>

</table>
</div>
<%= link_to 'Enter another URL', '/', :html => {:class => 'nav'} %>
</div>

